{
    "manifest_version": 3,
    "version": "1.0",
   
    "name": "Pixel Auto-Craft",
    "content_scripts": [
        {
           "matches": ["<all_urls>"],
           "js": ["content.js"]
        }
     ],
     "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
     ],
     "background" : {
        "service_worker": "./background.js"
     },
     "action": {
      "default_popup": "index.html"
     }
   
     
}